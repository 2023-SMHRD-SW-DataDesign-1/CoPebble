<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.mapper.DiaryMapper">

	<!-- insert into calendar VALUES(1,'test0001','CALENAR','TEST01','집가자','2023-07-05T00:00:00.000Z','2023-07-07T00:00:00.000Z','#ffdd7c');  -->
	<insert id="updateCalender" parameterType="calendarDTO">
		insert into calendar VALUES(#{NUM},#{Family_key},#{ASSORT},#{WHO},#{TITLE},#{S_DATE},#{E_DATE},#{COLOR})
	</insert>

	<select id="selectCalender">
		select * from calendar where Family_key = #{Family_key}
	</select>

	
	<delete id="deleteCalender" parameterType="int">
		DELETE from calendar where Family_key = #{Family_key}
	</delete>
</mapper>